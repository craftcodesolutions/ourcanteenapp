{
  "expo": {
    "name": "OurCanteen",
    "slug": "OurCanteen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/smallogo.png",
    "scheme": "ourcanteen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/smallogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fahaddhruba.OurCanteen"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ourcanteenicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#822723"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3cd0a83e-aa9e-40b1-b2f6-66fc17caaccd"
      }
    }
  }
}
